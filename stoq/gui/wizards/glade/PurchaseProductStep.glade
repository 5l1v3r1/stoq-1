                                                <child>
                                                    <widget class="GtkHBox" id="GtkHBox1">
                                                        <property name="visible">True</property>
                                                        <child>
                                                            <widget class="GtkImage" id="GtkImage1">
                                                                <property name="file">search16px.png</property>
