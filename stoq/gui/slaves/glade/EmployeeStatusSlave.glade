<?xml version="1.0" ?>
<glade-interface>
    <requires lib="kiwi"/>
    <widget class="GtkWindow" id="EmployeeStatusSlave">
        <property name="title" context="True" translatable="yes"></property>
        <property name="role"></property>
        <property name="events"></property>
        <property name="can_focus">True</property>
        <child>
            <widget class="GtkHBox" id="hbox1">
                <property name="events"></property>
                <property name="spacing">5</property>
                <property name="visible">True</property>
                <child>
                    <widget class="GtkLabel" id="label1">
                        <property name="label" context="True" translatable="yes">Status:</property>
                        <property name="events"></property>
                        <property name="xalign">1.0</property>
                        <property name="visible">True</property>
                    </widget>
                    <packing/>
                </child>
                <child>
                    <widget class="kiwi+ui+widgets+radiobutton+RadioButton" id="normal">
                        <property name="model_attribute">status</property>
                        <property name="data_value">0</property>
                        <property name="label" context="True" translatable="yes">Normal</property>
                        <property name="events"></property>
                        <property name="active">True</property>
                        <property name="can_focus">True</property>
                        <property name="data_type">int</property>
                        <property name="draw_indicator">True</property>
                        <property name="visible">True</property>
                        <property name="label">Normal</property>
                    </widget>
                    <packing>
                        <property name="position">1</property>
                    </packing>
                </child>
                <child>
                    <widget class="kiwi+ui+widgets+radiobutton+RadioButton" id="vacation">
                        <property name="model_attribute">status</property>
                        <property name="data_value">2</property>
                        <property name="group">normal</property>
                        <property name="label" context="True" translatable="yes">Vacation</property>
                        <property name="events"></property>
                        <property name="can_focus">True</property>
                        <property name="data_type">int</property>
                        <property name="draw_indicator">True</property>
                        <property name="visible">True</property>
                        <property name="label">Vacation</property>
                    </widget>
                    <packing>
                        <property name="position">2</property>
                    </packing>
                </child>
                <child>
                    <widget class="kiwi+ui+widgets+radiobutton+RadioButton" id="away">
                        <property name="model_attribute">status</property>
                        <property name="data_value">1</property>
                        <property name="group">normal</property>
                        <property name="label" context="True" translatable="yes">Away</property>
                        <property name="events"></property>
                        <property name="can_focus">True</property>
                        <property name="data_type">int</property>
                        <property name="draw_indicator">True</property>
                        <property name="visible">True</property>
                        <property name="label">Away</property>
                    </widget>
                    <packing>
                        <property name="position">3</property>
                    </packing>
                </child>
                <child>
                    <widget class="kiwi+ui+widgets+radiobutton+RadioButton" id="off">
                        <property name="model_attribute">status</property>
                        <property name="data_value">3</property>
                        <property name="group">normal</property>
                        <property name="label" context="True" translatable="yes">Desligado</property>
                        <property name="events"></property>
                        <property name="can_focus">True</property>
                        <property name="data_type">int</property>
                        <property name="draw_indicator">True</property>
                        <property name="visible">True</property>
                        <property name="label">Off</property>
                    </widget>
                    <packing>
                        <property name="position">4</property>
                    </packing>
                </child>
            </widget>
        </child>
    </widget>
</glade-interface>
