<?xml version="1.0" ?>
<glade-interface>
    <requires lib="kiwi"/>
    <widget class="GtkWindow" id="SupplierDetailsSlave">
        <property name="title" context="True" translatable="True"></property>
        <property name="role"></property>
        <property name="events"></property>
        <property name="can_focus">True</property>
        <child>
            <widget class="GtkTable" id="table1">
                <property name="row_spacing">2</property>
                <property name="column_spacing">2</property>
                <property name="border_width">5</property>
                <property name="events"></property>
                <property name="n_rows">2</property>
                <property name="n_columns">4</property>
                <property name="visible">True</property>
                <child>
                    <widget class="GtkLabel" id="label1">
                        <property name="label" context="True" translatable="True">Status:</property>
                        <property name="events"></property>
                        <property name="xalign">1.0</property>
                        <property name="visible">True</property>
                    </widget>
                    <packing>
                        <property name="y_options"></property>
                        <property name="x_options">fill</property>
                    </packing>
                </child>
                <child>
                    <widget class="kiwi+ui+widgets+radiobutton+RadioButton" id="active">
