<?xml version="1.0" standalone="no"?> <!--*- mode: xml -*-->
<!DOCTYPE glade-interface SYSTEM "http://gazpacho.sicem.biz/gazpacho-0.1.dtd">
<glade-interface>
    <widget class="GtkWindow" id="UserProfileSettingsSlave">
        <property name="default_height">250</property>
        <property name="default_width">440</property>
        <child>
            <widget class="GtkHBox" id="hbox1">
                <property name="border_width">5</property>
                <property name="spacing">5</property>
                <property name="visible">True</property>
                <child>
                    <widget class="kiwi+ui+widgets+label+Label" id="profile_name">
                        <property name="data_type">str</property>
                        <property name="xalign">1.0</property>
                    </widget>
                </child>
                <child>
                    <widget class="kiwi+ui+widgets+checkbutton+CheckButton" id="has_permission">
                        <property name="active">True</property>
                        <property name="data_type">bool</property>
                        <property name="model_attribute">has_permission</property>
                    </widget>
                    <packing>
                        <property name="expand">False</property>
                        <property name="position">1</property>
                    </packing>
                </child>
            </widget>
        </child>
    </widget>
</glade-interface>
