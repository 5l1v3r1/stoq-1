<?xml version="1.0" ?>
<glade-interface>
    <widget class="GtkWindow" id="BaseSellableCategoryDataSlave">
        <property name="events"></property>
        <child>
            <widget class="GtkHBox" id="hbox1">
                <property name="events"></property>
                <property name="spacing">5</property>
                <property name="visible">True</property>
                <child>
                    <widget class="GtkLabel" id="label1">
                        <property name="label" context="True" translatable="yes">Description: </property>
                        <property name="events"></property>
                        <property name="xalign">1.0</property>
                        <property name="visible">True</property>
                    </widget>
                    <packing>
                        <property name="expand">False</property>
                    </packing>
                </child>
                <child>
                    <widget class="kiwi+ui+widgets+entry+Entry" id="description">
                        <property name="mandatory">True</property>
                        <property name="model_attribute">category_data.description</property>
                        <property name="events"></property>
                        <property name="can_focus">True</property>
                        <property name="data_type">str</property>
                        <property name="visible">True</property>
                    </widget>
                    <packing>
                        <property name="position">1</property>
                    </packing>
                </child>
                <child>
                    <widget class="GtkLabel" id="label2">
                        <property name="label" context="True" translatable="yes">Suggested Markup (%):</property>
                        <property name="events"></property>
                        <property name="xalign">1.0</property>
                        <property name="visible">True</property>
                    </widget>
                    <packing>
                        <property name="position">2</property>
                        <property name="expand">False</property>
                    </packing>
                </child>
                <child>
                    <widget class="kiwi+ui+widgets+spinbutton+SpinButton" id="markup">
                        <property name="adjustment">0.000000 0.000000 100.000000 1.000000 10.000000 0.000000</property>
                        <property name="model_attribute">category_data.suggested_markup</property>
                        <property name="text" context="True" translatable="yes">0.00</property>
                        <property name="events"></property>
                        <property name="digits">2</property>
                        <property name="can_focus">True</property>
                        <property name="data_type">float</property>
                        <property name="visible">True</property>
                    </widget>
                    <packing>
                        <property name="position">3</property>
                        <property name="expand">False</property>
                    </packing>
                </child>
            </widget>
        </child>
    </widget>
</glade-interface>
