<?xml version="1.0" ?>
<glade-interface>
    <widget class="GtkWindow" id="SellableCategoryDataSlave">
        <property name="events"></property>
        <child>
            <widget class="GtkTable" id="table1">
                <property name="row_spacing">5</property>
                <property name="column_spacing">5</property>
                <property name="events"></property>
                <property name="n_rows">2</property>
                <property name="n_columns">4</property>
                <property name="visible">True</property>
                <child>
                    <widget class="kiwi+ui+widgets+entry+Entry" id="description">
                        <property name="mandatory">True</property>
                        <property name="model_attribute">category_data.description</property>
                        <property name="events"></property>
                        <property name="can_focus">True</property>
                        <property name="data_type">str</property>
                        <property name="visible">True</property>
                    </widget>
                    <packing>
                        <property name="left_attach">1</property>
                        <property name="y_options">fill</property>
                        <property name="right_attach">2</property>
                    </packing>
                </child>
                <child>
                    <widget class="GtkLabel" id="label4">
                        <property name="label" context="True" translatable="True">Description: </property>
                        <property name="events"></property>
                        <property name="xalign">1.0</property>
                        <property name="visible">True</property>
                    </widget>
                    <packing>
                        <property name="y_options">fill</property>
                        <property name="x_options">fill</property>
                    </packing>
                </child>
                <child>
                    <widget class="GtkLabel" id="label5">
                        <property name="label" context="True" translatable="True">Suggested Markup (%):</property>
                        <property name="events"></property>
                        <property name="xalign">1.0</property>
                        <property name="visible">True</property>
                    </widget>
                    <packing>
                        <property name="left_attach">2</property>
                        <property name="y_options">fill</property>
                        <property name="x_options">fill</property>
                        <property name="right_attach">3</property>
                    </packing>
                </child>
                <child>
                    <widget class="GtkLabel" id="label7">
                        <property name="label" context="True" translatable="True">Base Category:</property>
                        <property name="events"></property>
                        <property name="xalign">1.0</property>
                        <property name="visible">True</property>
                    </widget>
                    <packing>
                        <property name="bottom_attach">2</property>
                        <property name="y_options">fill</property>
                        <property name="x_options">fill</property>
                        <property name="top_attach">1</property>
                    </packing>
                </child>
                <child>
                    <widget class="kiwi+ui+widgets+spinbutton+SpinButton" id="suggested_markup">
                        <property name="adjustment">0.000000 0.000000 100.000000 1.000000 10.000000 0.000000</property>
                        <property name="model_attribute">category_data.suggested_markup</property>
                        <property name="text" context="True" translatable="True">0.00</property>
                        <property name="events"></property>
                        <property name="digits">2</property>
                        <property name="can_focus">True</property>
                        <property name="data_type">float</property>
                        <property name="visible">True</property>
                    </widget>
                    <packing>
                        <property name="left_attach">3</property>
                        <property name="y_options">fill</property>
                        <property name="right_attach">4</property>
                    </packing>
                </child>
                <child>
                    <widget class="kiwi+ui+widgets+combobox+ComboBoxEntry" id="base_category">
                        <property name="mandatory">True</property>
                        <property name="model_attribute">base_category</property>
                        <property name="events"></property>
                        <property name="data_type">object</property>
                        <property name="visible">True</property>
                    </widget>
                    <packing>
                        <property name="left_attach">1</property>
                        <property name="bottom_attach">2</property>
                        <property name="y_options">fill</property>
                        <property name="top_attach">1</property>
                        <property name="right_attach">4</property>
                    </packing>
                </child>
            </widget>
        </child>
    </widget>
</glade-interface>
