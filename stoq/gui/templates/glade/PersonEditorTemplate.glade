<?xml version="1.0" ?>
<glade-interface>
    <requires lib="gtk+"/>
    <requires lib="kiwi"/>
    <widget class="GtkWindow" id="PersonEditorTemplate">
        <property name="border_width">3</property>
        <property name="resize_mode">queue</property>
        <property name="role"></property>
        <property name="title" context="True" translatable="True"></property>
        <property name="type">toplevel</property>
        <child>
            <widget class="GtkNotebook" id="notebook">
                <property name="border_width">5</property>
                <property name="can_focus">True</property>
                <property name="tab_hborder">5</property>
                <property name="tab_vborder">3</property>
                <property name="visible">True</property>
                <child>
                    <widget class="GtkVBox" id="vbox1">
                        <property name="border_width">5</property>
                        <property name="spacing">5</property>
                        <property name="visible">True</property>
                        <child>
                            <widget class="GtkHBox" id="hbox1">
                                <property name="spacing">5</property>
                                <property name="visible">True</property>
                                <child>
                                    <widget class="GtkLabel" id="label1">
                                        <property name="label" context="True" translatable="True">Name</property>
                                        <property name="xalign">1.0</property>
                                        <property name="visible">True</property>
                                    </widget>
                                    <packing>
                                        <property name="expand">False</property>
                                    </packing>
                                </child>
                                <child>
                                    <widget class="kiwi+ui+widgets+entry+Entry" id="name">
                                        <property name="can_focus">True</property>
                                        <property name="completion">False</property>
                                        <property name="data_type">str</property>
                                        <property name="events">button-press-mask | button-release-mask | key-press-mask</property>
                                        <property name="mandatory">True</property>
                                        <property name="model_attribute">name</property>
                                        <property name="visible">True</property>
                                    </widget>
                                    <packing>
                                        <property name="position">1</property>
                                    </packing>
                                </child>
                            </widget>
                            <packing>
                                <property name="expand">False</property>
                                <property name="fill">False</property>
                            </packing>
                        </child>
                        <child>
                            <widget class="GtkFrame" id="id_frame">
                                <property name="label" context="True" translatable="True">Identification</property>
                                <property name="label_xalign">0.0</property>
                                <property name="visible">True</property>
                                <child>
                                    <widget class="GtkEventBox" id="id_holder">
                                        <property name="border_width">5</property>
                                        <property name="visible">True</property>
                                        <child>
                                            <placeholder/>
                                        </child>
                                    </widget>
                                </child>
                            </widget>
                            <packing>
                                <property name="position">1</property>
                                <property name="expand">False</property>
                            </packing>
                        </child>
                        <child>
                            <placeholder/>
                            <packing>
                                <property name="position">2</property>
                            </packing>
                        </child>
                        <child>
                            <widget class="GtkFrame" id="address_frame">
                                <property name="label" context="True" translatable="True">Main Address</property>
                                <property name="label_xalign">0.0</property>
                                <property name="visible">True</property>
                                <child>
                                    <widget class="GtkVBox" id="vbox3">
                                        <property name="border_width">5</property>
                                        <property name="spacing">5</property>
                                        <property name="visible">True</property>
                                        <child>
                                            <widget class="GtkEventBox" id="address_holder">
                                                <property name="visible">True</property>
                                                <child>
                                                    <placeholder/>
                                                </child>
                                            </widget>
                                            <packing/>
                                        </child>
                                        <child>
                                            <widget class="GtkHButtonBox" id="hbuttonbox1">
                                                <property name="layout_style">end</property>
                                                <property name="visible">True</property>
                                                <child>
                                                    <widget class="GtkButton" id="address_button">
                                                        <property name="can_focus">True</property>
                                                        <property name="label">More Addresses...</property>
                                                        <property name="receives_default">True</property>
                                                        <property name="visible">True</property>
                                                    </widget>
                                                    <packing/>
                                                </child>
                                            </widget>
                                            <packing>
                                                <property name="position">1</property>
                                                <property name="expand">False</property>
                                                <property name="fill">False</property>
                                            </packing>
                                        </child>
                                    </widget>
                                </child>
                            </widget>
                            <packing>
                                <property name="position">3</property>
                                <property name="expand">False</property>
                            </packing>
                        </child>
                    </widget>
                    <packing>
                        <property name="menu_label" translatable="True"></property>
                        <property name="tab_expand">False</property>
                        <property name="tab_label" translatable="True">Identification</property>
                    </packing>
                </child>
                <child>
                    <widget class="GtkVBox" id="vbox2">
                        <property name="visible">True</property>
                        <child>
                            <widget class="GtkTable" id="table2">
                                <property name="border_width">5</property>
                                <property name="column_spacing">5</property>
                                <property name="n_columns">4</property>
                                <property name="n_rows">2</property>
                                <property name="row_spacing">5</property>
                                <property name="visible">True</property>
                                <child>
                                    <widget class="kiwi+ui+widgets+entry+Entry" id="email">
                                        <property name="can_focus">True</property>
                                        <property name="completion">False</property>
                                        <property name="data_type">str</property>
                                        <property name="events">button-press-mask | button-release-mask | key-press-mask</property>
                                        <property name="model_attribute">email</property>
                                        <property name="visible">True</property>
                                    </widget>
                                    <packing>
                                        <property name="left_attach">3</property>
                                        <property name="bottom_attach">2</property>
                                        <property name="top_attach">1</property>
                                        <property name="right_attach">4</property>
                                    </packing>
                                </child>
                                <child>
                                    <widget class="GtkLabel" id="email_lbl">
                                        <property name="label" context="True" translatable="True">E-mail:</property>
                                        <property name="xalign">1.0</property>
                                        <property name="xpad">2</property>
                                        <property name="visible">True</property>
                                    </widget>
                                    <packing>
                                        <property name="left_attach">2</property>
                                        <property name="bottom_attach">2</property>
                                        <property name="x_options">fill</property>
                                        <property name="top_attach">1</property>
                                        <property name="right_attach">3</property>
                                    </packing>
                                </child>
                                <child>
                                    <widget class="kiwi+ui+widgets+entry+Entry" id="fax_number">
                                        <property name="can_focus">True</property>
                                        <property name="completion">False</property>
                                        <property name="data_type">str</property>
                                        <property name="events">button-press-mask | button-release-mask | key-press-mask</property>
                                        <property name="model_attribute">fax_number</property>
                                        <property name="visible">True</property>
                                    </widget>
                                    <packing>
                                        <property name="left_attach">1</property>
                                        <property name="bottom_attach">2</property>
                                        <property name="top_attach">1</property>
                                        <property name="right_attach">2</property>
                                    </packing>
                                </child>
                                <child>
                                    <widget class="GtkLabel" id="fax_lbl">
                                        <property name="label" context="True" translatable="True">Fax: </property>
                                        <property name="xalign">1.0</property>
                                        <property name="xpad">2</property>
                                        <property name="visible">True</property>
                                    </widget>
                                    <packing>
                                        <property name="bottom_attach">2</property>
                                        <property name="x_options">fill</property>
                                        <property name="top_attach">1</property>
                                    </packing>
                                </child>
                                <child>
                                    <widget class="kiwi+ui+widgets+entry+Entry" id="mobile_number">
                                        <property name="can_focus">True</property>
                                        <property name="completion">False</property>
                                        <property name="data_type">str</property>
                                        <property name="events">button-press-mask | button-release-mask | key-press-mask</property>
                                        <property name="model_attribute">mobile_number</property>
                                        <property name="visible">True</property>
                                    </widget>
                                    <packing>
                                        <property name="left_attach">3</property>
                                        <property name="right_attach">4</property>
                                    </packing>
                                </child>
                                <child>
                                    <widget class="GtkLabel" id="mobile_lbl">
                                        <property name="label" context="True" translatable="True">Mobile:</property>
                                        <property name="xalign">1.0</property>
                                        <property name="xpad">2</property>
                                        <property name="visible">True</property>
                                    </widget>
                                    <packing>
                                        <property name="left_attach">2</property>
                                        <property name="x_options">fill</property>
                                        <property name="right_attach">3</property>
                                    </packing>
                                </child>
                                <child>
                                    <widget class="kiwi+ui+widgets+entry+Entry" id="phone_number">
                                        <property name="can_focus">True</property>
                                        <property name="completion">False</property>
                                        <property name="data_type">str</property>
                                        <property name="events">button-press-mask | button-release-mask | key-press-mask</property>
                                        <property name="model_attribute">phone_number</property>
                                        <property name="visible">True</property>
                                    </widget>
                                    <packing>
                                        <property name="left_attach">1</property>
                                        <property name="right_attach">2</property>
                                    </packing>
                                </child>
                                <child>
                                    <widget class="GtkLabel" id="phone_number_lbl">
                                        <property name="label" context="True" translatable="True">Phone Number:</property>
                                        <property name="xalign">1.0</property>
                                        <property name="xpad">2</property>
                                        <property name="visible">True</property>
                                    </widget>
                                    <packing>
                                        <property name="x_options">fill</property>
                                    </packing>
                                </child>
                            </widget>
                            <packing>
                                <property name="expand">False</property>
                            </packing>
                        </child>
                        <child>
                            <widget class="GtkFrame" id="frame1">
                                <property name="border_width">5</property>
                                <property name="label" context="True" translatable="True">More Contacts</property>
                                <property name="label_xalign">0.0</property>
                                <property name="visible">True</property>
                                <child>
                                    <widget class="GtkEventBox" id="contact_list_holder">
                                        <property name="border_width">5</property>
                                        <property name="visible">True</property>
                                        <child>
                                            <placeholder/>
                                        </child>
                                    </widget>
                                </child>
                            </widget>
                            <packing>
                                <property name="position">1</property>
                            </packing>
                        </child>
                    </widget>
                    <packing>
                        <property name="position">1</property>
                        <property name="tab_expand">False</property>
                        <property name="tab_label" translatable="True">Contact</property>
                    </packing>
                </child>
                <child>
                    <widget class="GtkVBox" id="vbox4">
                        <property name="visible">True</property>
                        <child>
                            <widget class="GtkEventBox" id="person_status_holder">
                                <property name="visible">True</property>
                                <child>
                                    <placeholder/>
                                </child>
                            </widget>
                            <packing>
                                <property name="expand">False</property>
                            </packing>
                        </child>
                        <child>
                            <widget class="GtkEventBox" id="details_holder">
                                <property name="visible">True</property>
                                <child>
                                    <placeholder/>
                                </child>
                            </widget>
                            <packing>
                                <property name="position">1</property>
                                <property name="expand">False</property>
                            </packing>
                        </child>
                        <child>
                            <widget class="GtkEventBox" id="note_holder">
                                <property name="visible">True</property>
                                <child>
                                    <placeholder/>
                                </child>
                            </widget>
                            <packing>
                                <property name="position">2</property>
                            </packing>
                        </child>
                    </widget>
                    <packing>
                        <property name="menu_label" translatable="True"></property>
                        <property name="position">2</property>
                        <property name="tab_expand">False</property>
                        <property name="tab_label" translatable="True">Details</property>
                    </packing>
                </child>
                <child>
                    <widget class="GtkEventBox" id="custom_holder">
                        <property name="no_show_all">True</property>
                        <child>
                            <placeholder/>
                        </child>
                    </widget>
                    <packing>
                        <property name="menu_label" translatable="True"></property>
                        <property name="position">3</property>
                        <property name="tab_expand">False</property>
                        <property name="tab_label" translatable="True">Customizable</property>
                    </packing>
                </child>
            </widget>
        </child>
    </widget>
</glade-interface>
