<?xml version="1.0" ?>
<glade-interface>
    <widget class="GtkWindow" id="SellableSearchHeader">
        <property name="events"></property>
        <child>
            <widget class="GtkHBox" id="hbox1">
                <property name="events"></property>
                <property name="spacing">5</property>
                <property name="visible">True</property>
                <child>
                    <widget class="kiwi+ui+widgets+label+Label" id="sellable_description">
                        <property name="label" context="True" translatable="yes">No item selected</property>
                        <property name="use_markup">True</property>
                        <property name="events"></property>
                        <property name="xalign">0.0</property>
                        <property name="data_type">str</property>
                        <property name="visible">True</property>
                    </widget>
                    <packing/>
                </child>
                <child>
                    <widget class="kiwi+ui+widgets+label+Label" id="price">
                        <property name="label" context="True" translatable="yes">0,00</property>
                        <property name="use_markup">True</property>
                        <property name="events"></property>
                        <property name="xalign">1.0</property>
                        <property name="data_type">str</property>
                        <property name="visible">True</property>
                    </widget>
                    <packing>
                        <property name="position">2</property>
                        <property name="expand">False</property>
                    </packing>
                </child>
            </widget>
        </child>
    </widget>
</glade-interface>
