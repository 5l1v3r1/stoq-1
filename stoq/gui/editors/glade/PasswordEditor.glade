<?xml version="1.0" standalone="no"?> <!--*- mode: xml -*-->
<!DOCTYPE glade-interface SYSTEM "http://gazpacho.sicem.biz/gazpacho-0.1.dtd">
<glade-interface>
    <widget class="GtkWindow" id="PasswordEditor">
        <property name="default_height">250</property>
        <property name="default_width">440</property>
        <child>
            <widget class="GtkTable" id="table2">
                <property name="border_width">5</property>
                <property name="column_spacing">6</property>
                <property name="n_columns">2</property>
                <property name="n_rows">2</property>
                <property name="visible">True</property>
                <child>
                    <widget class="GtkLabel" id="current_password_lbl">
                        <property name="label" context="yes" translatable="yes">Current Password:</property>
                        <property name="visible">True</property>
                        <property name="xalign">1.0</property>
                    </widget>
                    <packing>
                        <property name="x_options">fill</property>
                        <property name="y_options">fill</property>
                    </packing>
                </child>
                <child>
                    <widget class="GtkEventBox" id="password_holder">
                        <property name="border_width">5</property>
                        <property name="visible">True</property>
                        <child>
                            <placeholder/>
                        </child>
                    </widget>
                    <packing>
                        <property name="bottom_attach">2</property>
                        <property name="right_attach">2</property>
                        <property name="top_attach">1</property>
                        <property name="x_options">fill</property>
                        <property name="y_options">fill</property>
                    </packing>
                </child>
                <child>
                    <widget class="kiwi+ui+widgets+entry+Entry" id="current_password">
                        <property name="data_type">str</property>
                        <property name="is_focus">True</property>
                        <property name="mandatory">True</property>
                        <property name="model_attribute">current_password</property>
                        <property name="visibility">False</property>
                    </widget>
                    <packing>
                        <property name="left_attach">1</property>
                        <property name="right_attach">2</property>
                        <property name="x_options">fill</property>
                        <property name="y_options">fill</property>
                    </packing>
                </child>
                <child>
                    <placeholder/>
                    <packing>
                        <property name="left_attach">1</property>
                        <property name="right_attach">2</property>
                        <property name="top_attach">1</property>
                        <property name="bottom_attach">2</property>
                        <property name="x_options">&lt;flags 0 of type GtkAttachOptions&gt;</property>
                        <property name="y_options">&lt;flags 0 of type GtkAttachOptions&gt;</property>
                    </packing>
                </child>
            </widget>
        </child>
    </widget>
</glade-interface>
