<?xml version="1.0" standalone="no"?> <!--*- mode: xml -*-->
<!DOCTYPE glade-interface SYSTEM "http://gazpacho.sicem.biz/gazpacho-0.1.dtd">
<glade-interface domain="stoqlib">
    <widget class="GtkWindow" id="AddressEditor">
        <child>
            <widget class="GtkVBox" id="vbox1">
                <property name="border_width">5</property>
                <property name="spacing">5</property>
                <property name="visible">True</property>
                <child>
                    <widget class="GtkEventBox" id="main_holder">
                        <property name="visible">True</property>
                        <child>
                            <placeholder/>
                        </child>
                    </widget>
                </child>
                <child>
                    <widget class="kiwi+ui+widgets+checkbutton+CheckButton" id="is_main_address_checkbutton">
                        <property name="data_type">bool</property>
                        <property name="is_focus">True</property>
                        <property name="label" context="yes" translatable="yes">Main address</property>
                        <property name="model_attribute">is_main_address</property>
                        <property name="visible">True</property>
                    </widget>
                    <packing>
                        <property name="expand">False</property>
                        <property name="position">1</property>
                    </packing>
                </child>
            </widget>
        </child>
    </widget>
</glade-interface>
