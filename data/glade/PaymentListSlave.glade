<?xml version="1.0" standalone="no"?> <!--*- mode: xml -*-->
<!DOCTYPE glade-interface SYSTEM "http://gazpacho.sicem.biz/gazpacho-0.1.dtd">
<glade-interface>
    <widget class="GtkWindow" id="PaymentListSlave">
        <property name="default_height">250</property>
        <property name="default_width">440</property>
        <property name="mnemonics_visible">False</property>
        <child>
            <widget class="GtkVBox" id="vbox1">
                <property name="border_width">5</property>
                <property name="spacing">5</property>
                <property name="visible">True</property>
                <child>
                    <widget class="ObjectList" id="payment_list">
                        <property name="border_width">5</property>
                        <property name="selection_mode">single</property>
                        <property name="visible">True</property>
                    </widget>
                </child>
                <child>
                    <widget class="GtkHBox" id="hbox1">
                        <property name="border_width">5</property>
                        <property name="spacing">5</property>
                        <property name="visible">True</property>
                        <child>
                            <widget class="ProxyLabel" id="difference_status_label">
                                <property name="data_type">str</property>
                                <property name="label" context="yes" translatable="yes">Outstanding:</property>
                                <property name="model_attribute">difference_status_label</property>
                                <property name="visible">True</property>
                                <property name="xalign">1.0</property>
                            </widget>
                        </child>
                        <child>
                            <widget class="ProxyLabel" id="difference_label">
                                <property name="data_type">currency</property>
                                <property name="label" context="yes" translatable="yes">$0.00</property>
                                <property name="model_attribute">difference_label</property>
                                <property name="visible">True</property>
                                <property name="xalign">1.0</property>
                            </widget>
                            <packing>
                                <property name="expand">False</property>
                                <property name="position">1</property>
                            </packing>
                        </child>
                    </widget>
                    <packing>
                        <property name="expand">False</property>
                        <property name="fill">False</property>
                        <property name="position">1</property>
                    </packing>
                </child>
                <child>
                    <widget class="GtkHBox" id="hbox2">
                        <property name="border_width">5</property>
                        <property name="spacing">5</property>
                        <property name="visible">True</property>
                        <child>
                            <widget class="ProxyLabel" id="total_status_label">
                                <property name="label" context="yes" translatable="yes">Total:</property>
                                <property name="model_attribute">total_status_label</property>
                                <property name="visible">True</property>
                                <property name="xalign">1.0</property>
                            </widget>
                        </child>
                        <child>
                            <widget class="ProxyLabel" id="total_label">
                                <property name="data_type">currency</property>
                                <property name="label" context="yes" translatable="yes">$0.00</property>
                                <property name="model_attribute">total_label</property>
                                <property name="visible">True</property>
                                <property name="xalign">1.0</property>
                            </widget>
                            <packing>
                                <property name="expand">False</property>
                                <property name="position">1</property>
                            </packing>
                        </child>
                    </widget>
                    <packing>
                        <property name="expand">False</property>
                        <property name="fill">False</property>
                        <property name="position">2</property>
                    </packing>
                </child>
            </widget>
        </child>
    </widget>
</glade-interface>
