<?xml version="1.0" standalone="no"?> <!--*- mode: xml -*-->
<!DOCTYPE glade-interface SYSTEM "http://gazpacho.sicem.biz/gazpacho-0.1.dtd">
<glade-interface domain="stoqlib">
    <widget class="GtkWindow" id="DebitCardDetailsStep">
        <property name="default_height">250</property>
        <property name="default_width">440</property>
        <child>
            <widget class="GtkHBox" id="hbox1">
                <property name="border_width">6</property>
                <property name="spacing">6</property>
                <property name="visible">True</property>
                <child>
                    <widget class="GtkLabel" id="label1">
                        <property name="label" context="yes" translatable="yes">Receive Payments in:</property>
                        <property name="visible">True</property>
                        <property name="xalign">1.0</property>
                    </widget>
                </child>
                <child>
                    <widget class="kiwi+ui+widgets+spinbutton+SpinButton" id="receive_days">
                        <property name="adjustment">0 0 100 1 10 0</property>
                        <property name="data_type">int</property>
                        <property name="model_attribute">receive_days</property>
                        <property name="text" context="yes" translatable="yes">0</property>
                        <property name="visible">True</property>
                        <property name="width_chars">4</property>
                        <property name="xalign">1.0</property>
                    </widget>
                    <packing>
                        <property name="expand">False</property>
                        <property name="position">1</property>
                    </packing>
                </child>
                <child>
                    <widget class="GtkLabel" id="label2">
                        <property name="label" context="yes" translatable="yes">days</property>
                        <property name="visible">True</property>
                        <property name="xalign">0.0</property>
                    </widget>
                    <packing>
                        <property name="position">2</property>
                    </packing>
                </child>
            </widget>
        </child>
    </widget>
</glade-interface>
