<?xml version="1.0" standalone="no"?> <!--*- mode: xml -*-->
<!DOCTYPE glade-interface SYSTEM "http://gazpacho.sicem.biz/gazpacho-0.1.dtd">
<glade-interface domain="stoq">
    <widget class="GtkWindow" id="SelectApplicationsSlave">
        <property name="border_width">6</property>
        <property name="default_height">250</property>
        <property name="default_width">440</property>
        <child>
            <widget class="GtkHBox" id="hbox1">
                <property name="visible">True</property>
                <child>
                    <widget class="GtkVBox" id="app_list">
                        <property name="border_width">6</property>
                        <property name="spacing">6</property>
                        <property name="visible">True</property>
                        <child>
                            <widget class="ProxyLabel" id="kiwilabel1">
                                <property name="label" context="yes" translatable="yes">Select an Application:</property>
                                <property name="visible">True</property>
                                <property name="xalign">0.0</property>
                            </widget>
                            <packing>
                                <property name="expand">False</property>
                                <property name="padding">6</property>
                            </packing>
                        </child>
                        <child>
                            <widget class="ObjectList" id="klist">
                                <property name="visible">True</property>
                                <property name="vscrollbar_policy">automatic</property>
                            </widget>
                            <packing>
                                <property name="position">1</property>
                            </packing>
                        </child>
                    </widget>
                    <packing>
                        <property name="padding">6</property>
                    </packing>
                </child>
                <child>
                    <widget class="GtkVBox" id="vbox1">
                        <property name="border_width">6</property>
                        <property name="visible">True</property>
                        <child>
                            <widget class="GtkImage" id="logo">
                                <property name="visible">True</property>
                            </widget>
                            <packing>
                                <property name="expand">False</property>
                                <property name="fill">False</property>
                            </packing>
                        </child>
                        <child>
                            <widget class="GtkLabel" id="description">
                                <property name="label" context="yes" translatable="yes"></property>
                                <property name="visible">True</property>
                                <property name="wrap">True</property>
                                <property name="xalign">0.1</property>
                                <property name="yalign">0.1</property>
                                <property name="width_chars">35</property>
                            </widget>
                            <packing>
                                <property name="position">1</property>
                            </packing>
                        </child>
                    </widget>
                    <packing>
                        <property name="position">1</property>
                    </packing>
                </child>
            </widget>
        </child>
    </widget>
</glade-interface>
