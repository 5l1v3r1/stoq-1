<?xml version="1.0" standalone="no"?> <!--*- mode: xml -*-->
<!DOCTYPE glade-interface SYSTEM "http://gazpacho.sicem.biz/gazpacho-0.1.dtd">
<glade-interface domain="stoqlib">
    <widget class="GtkWindow" id="ImageHolder">
        <property name="default_height">1</property>
        <property name="default_width">1</property>
        <child>
            <widget class="GtkHBox" id="hbox1">
                <property name="visible">True</property>
                <child>
                    <widget class="GtkVSeparator" id="separator">
                        <property name="visible">True</property>
                    </widget>
                    <packing>
                        <property name="expand">False</property>
                        <property name="fill">False</property>
                    </packing>
                </child>
                <child>
                    <widget class="GtkEventBox" id="eventbox">
                        <property name="border_width">6</property>
                        <property name="visible">True</property>
                        <child>
                            <widget class="GtkImage" id="image">
                                <property name="height_request">64</property>
                                <property name="visible">True</property>
                                <property name="width_request">64</property>
                                <property name="xpad">6</property>
                            </widget>
                        </child>
                    </widget>
                    <packing>
                        <property name="position">1</property>
                    </packing>
                </child>
            </widget>
        </child>
    </widget>
</glade-interface>
