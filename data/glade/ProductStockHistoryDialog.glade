<?xml version="1.0" standalone="no"?> <!--*- mode: xml -*-->
<!DOCTYPE glade-interface SYSTEM "http://gazpacho.sicem.biz/gazpacho-0.1.dtd">
<glade-interface domain="stoqlib">
    <widget class="GtkWindow" id="ProductStockHistoryDialog">
        <property name="border_width">6</property>
        <property name="default_height">250</property>
        <property name="default_width">440</property>
        <child>
            <widget class="GtkVBox" id="vbox1">
                <property name="spacing">6</property>
                <property name="visible">True</property>
                <child>
                    <widget class="GtkTable" id="table1">
                        <property name="border_width">6</property>
                        <property name="column_spacing">6</property>
                        <property name="n_columns">3</property>
                        <property name="n_rows">2</property>
                        <property name="row_spacing">6</property>
                        <property name="visible">True</property>
                        <child>
                            <widget class="GtkLabel" id="productstockhistory">
                                <property name="label" context="yes" translatable="yes">Product:</property>
                                <property name="visible">True</property>
                                <property name="xalign">1.0</property>
                            </widget>
                        </child>
                        <child>
                            <widget class="GtkLabel" id="label2">
                                <property name="label" context="yes" translatable="yes">Current Stock Balance:</property>
                                <property name="visible">True</property>
                                <property name="xalign">1.0</property>
                            </widget>
                            <packing>
                                <property name="bottom_attach">2</property>
                                <property name="top_attach">1</property>
                            </packing>
                        </child>
                        <child>
                            <widget class="ProxyLabel" id="description">
                                <property name="data_type">unicode</property>
                                <property name="model_attribute">description</property>
                                <property name="visible">True</property>
                                <property name="xalign">0.0</property>
                            </widget>
                            <packing>
                                <property name="left_attach">1</property>
                                <property name="right_attach">2</property>
                            </packing>
                        </child>
                        <child>
                            <widget class="ProxyLabel" id="full_balance">
                                <property name="data_type">Decimal</property>
                                <property name="model_attribute">full_balance</property>
                                <property name="visible">True</property>
                                <property name="xalign">0.0</property>
                            </widget>
                            <packing>
                                <property name="bottom_attach">2</property>
                                <property name="left_attach">1</property>
                                <property name="right_attach">2</property>
                                <property name="top_attach">1</property>
                            </packing>
                        </child>
                        <child>
                            <widget class="GtkLabel" id="label1">
                                <property name="visible">True</property>
                                <property name="xalign">0.0</property>
                            </widget>
                            <packing>
                                <property name="left_attach">2</property>
                                <property name="right_attach">3</property>
                            </packing>
                        </child>
                        <child>
                            <widget class="GtkLabel" id="label3">
                                <property name="visible">True</property>
                                <property name="xalign">0.0</property>
                            </widget>
                            <packing>
                                <property name="bottom_attach">2</property>
                                <property name="left_attach">2</property>
                                <property name="right_attach">3</property>
                                <property name="top_attach">1</property>
                            </packing>
                        </child>
                    </widget>
                    <packing>
                        <property name="expand">False</property>
                    </packing>
                </child>
                <child>
                    <widget class="GtkNotebook" id="notebook1">
                        <property name="border_width">6</property>
                        <property name="is_focus">True</property>
                        <property name="visible">True</property>
                        <child>
                            <widget class="GtkVBox" id="receiving_vbox">
                                <property name="visible">True</property>
                                <child>
                                    <widget class="ObjectList" id="receiving_list">
                                        <property name="visible">True</property>
                                    </widget>
                                </child>
                                <child>
                                    <placeholder/>
                                    <packing>
                                        <property name="position">1</property>
                                    </packing>
                                </child>
                            </widget>
                            <packing>
                                <property name="tab_label" context="yes" translatable="yes">Receiving</property>
                            </packing>
                        </child>
                        <child>
                            <widget class="GtkVBox" id="sales_vbox">
                                <property name="visible">True</property>
                                <child>
                                    <widget class="ObjectList" id="sales_list">
                                        <property name="visible">True</property>
                                    </widget>
                                </child>
                                <child>
                                    <placeholder/>
                                    <packing>
                                        <property name="position">1</property>
                                    </packing>
                                </child>
                            </widget>
                            <packing>
                                <property name="position">1</property>
                                <property name="tab_label" context="yes" translatable="yes">Sales</property>
                            </packing>
                        </child>
                        <child>
                            <widget class="GtkVBox" id="transfer_vbox">
                                <property name="visible">True</property>
                                <child>
                                    <widget class="ObjectList" id="transfer_list">
                                        <property name="visible">True</property>
                                    </widget>
                                </child>
                                <child>
                                    <placeholder/>
                                    <packing>
                                        <property name="position">1</property>
                                    </packing>
                                </child>
                            </widget>
                            <packing>
                                <property name="position">2</property>
                                <property name="tab_label" context="yes" translatable="yes">Transfer</property>
                            </packing>
                        </child>
                        <child>
                            <widget class="GtkVBox" id="retention_vbox">
                                <property name="visible">True</property>
                                <child>
                                    <widget class="ObjectList" id="retention_list">
                                        <property name="visible">True</property>
                                    </widget>
                                </child>
                                <child>
                                    <placeholder/>
                                    <packing>
                                        <property name="position">1</property>
                                    </packing>
                                </child>
                            </widget>
                            <packing>
                                <property name="position">3</property>
                                <property name="tab_label" context="yes" translatable="yes">Retention</property>
                            </packing>
                        </child>
                    </widget>
                    <packing>
                        <property name="position">1</property>
                    </packing>
                </child>
            </widget>
        </child>
    </widget>
</glade-interface>
