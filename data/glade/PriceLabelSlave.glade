<?xml version="1.0" standalone="no"?> <!--*- mode: xml -*-->
<!DOCTYPE glade-interface SYSTEM "http://gazpacho.sicem.biz/gazpacho-0.1.dtd">
<glade-interface>
    <widget class="GtkWindow" id="PriceLabelSlave">
        <property name="default_height">250</property>
        <property name="default_width">440</property>
        <child>
            <widget class="GtkHBox" id="hbox1">
                <property name="spacing">5</property>
                <property name="visible">True</property>
                <child>
                    <widget class="ProxyLabel" id="label1">
                        <property name="label" context="yes" translatable="yes">Price ($CURRENCY):</property>
                        <property name="visible">True</property>
                        <property name="xalign">1.0</property>
                    </widget>
                    <packing>
                        <property name="expand">False</property>
                    </packing>
                </child>
                <child>
                    <widget class="kiwi+ui+widgets+label+Label" id="price">
                        <property name="data_type">currency</property>
                        <property name="ellipsize">end</property>
                        <property name="model_attribute">price</property>
                        <property name="visible">True</property>
                        <property name="width_chars">12</property>
                        <property name="xalign">0.0</property>
                    </widget>
                    <packing>
                        <property name="position">1</property>
                    </packing>
                </child>
            </widget>
        </child>
    </widget>
</glade-interface>
