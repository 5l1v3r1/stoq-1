<?xml version="1.0" standalone="no"?> <!--*- mode: xml -*-->
<!DOCTYPE glade-interface SYSTEM "http://gazpacho.sicem.biz/gazpacho-0.1.dtd">
<glade-interface domain="stoqlib">
    <widget class="GtkWindow" id="ProductSupplierEditor">
        <property name="destroy_with_parent">True</property>
        <property name="modal">True</property>
        <property name="window_position">center-on-parent</property>
        <child>
            <widget class="GtkTable" id="table1">
                <property name="border_width">6</property>
                <property name="column_spacing">6</property>
                <property name="n_columns">4</property>
                <property name="n_rows">3</property>
                <property name="row_spacing">6</property>
                <property name="visible">True</property>
                <child>
                    <widget class="GtkLabel" id="label2">
                        <property name="label" context="yes" translatable="yes">Supplier Cost:</property>
                        <property name="visible">True</property>
                        <property name="xalign">1.0</property>
                    </widget>
                    <packing>
                        <property name="y_options"></property>
                    </packing>
                </child>
                <child>
                    <widget class="GtkLabel" id="label1">
                        <property name="label" context="yes" translatable="yes">Lead Time:</property>
                        <property name="visible">True</property>
                        <property name="xalign">1.0</property>
                    </widget>
                    <packing>
                        <property name="bottom_attach">2</property>
                        <property name="top_attach">1</property>
                        <property name="y_options"></property>
                    </packing>
                </child>
                <child>
                    <widget class="GtkLabel" id="label3">
                        <property name="justify">right</property>
                        <property name="label" context="yes" translatable="yes">Additional 
Information:</property>
                        <property name="visible">True</property>
                        <property name="wrap">True</property>
                        <property name="xalign">1.0</property>
                        <property name="yalign">0.0</property>
                    </widget>
                    <packing>
                        <property name="bottom_attach">3</property>
                        <property name="top_attach">2</property>
                        <property name="x_options">fill</property>
                    </packing>
                </child>
                <child>
                    <widget class="ProxyEntry" id="base_cost">
                        <property name="data_type">currency</property>
                        <property name="is_focus">True</property>
                        <property name="mandatory">True</property>
                        <property name="model_attribute">base_cost</property>
                        <property name="visible">True</property>
                        <property name="width_chars">8</property>
                        <property name="xalign">1.0</property>
                    </widget>
                    <packing>
                        <property name="left_attach">1</property>
                        <property name="right_attach">2</property>
                        <property name="y_options"></property>
                    </packing>
                </child>
                <child>
                    <widget class="GtkHBox" id="hbox2">
                        <property name="spacing">6</property>
                        <property name="visible">True</property>
                        <child>
                            <widget class="ProxySpinButton" id="lead_time">
                                <property name="adjustment">1 0 100 1 10 0</property>
                                <property name="data_type">int</property>
                                <property name="is_focus">True</property>
                                <property name="mandatory">True</property>
                                <property name="model_attribute">lead_time</property>
                                <property name="text" context="yes" translatable="yes">1</property>
                                <property name="visible">True</property>
                            </widget>
                        </child>
                        <child>
                            <widget class="GtkLabel" id="label4">
                                <property name="label" context="yes" translatable="yes">Day(s)</property>
                                <property name="visible">True</property>
                                <property name="xalign">0.0</property>
                            </widget>
                            <packing>
                                <property name="expand">False</property>
                                <property name="position">1</property>
                            </packing>
                        </child>
                    </widget>
                    <packing>
                        <property name="bottom_attach">2</property>
                        <property name="left_attach">1</property>
                        <property name="right_attach">2</property>
                        <property name="top_attach">1</property>
                        <property name="y_options">fill</property>
                    </packing>
                </child>
                <child>
                    <widget class="GtkScrolledWindow" id="scrolledwindow1">
                        <property name="hscrollbar_policy">never</property>
                        <property name="shadow_type">in</property>
                        <property name="visible">True</property>
                        <property name="vscrollbar_policy">automatic</property>
                        <property name="window_placement">bottom-left</property>
                        <child>
                            <widget class="ProxyTextView" id="notes">
                                <property name="data_type">unicode</property>
                                <property name="left_margin">5</property>
                                <property name="model_attribute">notes</property>
                                <property name="right_margin">5</property>
                                <property name="visible">True</property>
                                <property name="width_request">400</property>
                                <property name="wrap_mode">word-char</property>
                            </widget>
                        </child>
                    </widget>
                    <packing>
                        <property name="bottom_attach">3</property>
                        <property name="left_attach">1</property>
                        <property name="right_attach">4</property>
                        <property name="top_attach">2</property>
                    </packing>
                </child>
                <child>
                    <widget class="ProxyLabel" id="label10">
                        <property name="label" context="yes" translatable="yes">ICMS:</property>
                        <property name="model_attribute"></property>
                        <property name="visible">True</property>
                        <property name="xalign">1.0</property>
                    </widget>
                    <packing>
                        <property name="left_attach">2</property>
                        <property name="right_attach">3</property>
                        <property name="y_options">fill</property>
                    </packing>
                </child>
                <child>
                    <widget class="GtkLabel" id="label5">
                        <property name="label" context="yes" translatable="yes">Minimum Purchase:</property>
                        <property name="visible">True</property>
                        <property name="xalign">1.0</property>
                    </widget>
                    <packing>
                        <property name="bottom_attach">2</property>
                        <property name="left_attach">2</property>
                        <property name="right_attach">3</property>
                        <property name="top_attach">1</property>
                        <property name="y_options"></property>
                    </packing>
                </child>
                <child>
                    <widget class="GtkHBox" id="hbox3">
                        <property name="spacing">6</property>
                        <property name="visible">True</property>
                        <child>
                            <widget class="ProxyEntry" id="icms">
                                <property name="data_type">Decimal</property>
                                <property name="model_attribute">icms</property>
                                <property name="visible">True</property>
                                <property name="width_chars">8</property>
                                <property name="xalign">1.0</property>
                            </widget>
                            <packing>
                                <property name="expand">False</property>
                            </packing>
                        </child>
                        <child>
                            <widget class="ProxyLabel" id="kiwilabel1">
                                <property name="label" context="yes" translatable="yes">%</property>
                                <property name="model_attribute">kiwilabel1</property>
                                <property name="visible">True</property>
                                <property name="xalign">0.0</property>
                            </widget>
                            <packing>
                                <property name="position">1</property>
                            </packing>
                        </child>
                    </widget>
                    <packing>
                        <property name="left_attach">3</property>
                        <property name="right_attach">4</property>
                    </packing>
                </child>
                <child>
                    <widget class="GtkHBox" id="hbox1">
                        <property name="spacing">6</property>
                        <property name="visible">True</property>
                        <child>
                            <widget class="ProxySpinButton" id="minimum_purchase">
                                <property name="adjustment">1 0 100 1 10 0</property>
                                <property name="data_type">Decimal</property>
                                <property name="digits">2</property>
                                <property name="is_focus">True</property>
                                <property name="mandatory">True</property>
                                <property name="model_attribute">minimum_purchase</property>
                                <property name="text" context="yes" translatable="yes">1.00</property>
                                <property name="visible">True</property>
                            </widget>
                        </child>
                        <child>
                            <widget class="ProxyLabel" id="unit_label">
                                <property name="model_attribute">unit_label</property>
                                <property name="visible">True</property>
                                <property name="xalign">0.0</property>
                            </widget>
                            <packing>
                                <property name="position">1</property>
                            </packing>
                        </child>
                    </widget>
                    <packing>
                        <property name="bottom_attach">2</property>
                        <property name="left_attach">3</property>
                        <property name="right_attach">4</property>
                        <property name="top_attach">1</property>
                        <property name="x_options">fill</property>
                        <property name="y_options">fill</property>
                    </packing>
                </child>
            </widget>
        </child>
    </widget>
</glade-interface>
